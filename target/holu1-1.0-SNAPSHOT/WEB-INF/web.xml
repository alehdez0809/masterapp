<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ModificarDatos</servlet-name>
        <servlet-class>Controller.ModificarDatos</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CerrarSesion</servlet-name>
        <servlet-class>Controller.CerrarSesion</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>BorrarClase</servlet-name>
        <servlet-class>Controller.BorrarClase</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnviarMensaje</servlet-name>
        <servlet-class>Controller.EnviarMensaje</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Calificar</servlet-name>
        <servlet-class>Controller.Calificar</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ModificarDatos</servlet-name>
        <url-pattern>/ModificarDatos</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CerrarSesion</servlet-name>
        <url-pattern>/CerrarSesion</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>BorrarClase</servlet-name>
        <url-pattern>/BorrarClase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnviarMensaje</servlet-name>
        <url-pattern>/EnviarMensaje</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Calificar</servlet-name>
        <url-pattern>/Calificar</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
